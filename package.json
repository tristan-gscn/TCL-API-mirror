{
  "name": "tcl-api-mirror",
  "version": "1.0.0",
  "type": "module",
  "description": "Mirror for real-time data of the Lyon TCL network",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "node --watch --loader ts-node/esm src/server.ts",
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "ts-node": {
    "esm": true,
    "experimentalSpecifierResolution": "node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tristan-gscn/TCL-API-mirror.git"
  },
  "keywords": [
    "tcl",
    "lyon",
    "sytral",
    "api"
  ],
  "author": "Dotshell",
  "license": "SEE LICENSE IN LICENSE.pdf",
  "bugs": {
    "url": "https://github.com/tristan-gscn/TCL-API-mirror/issues"
  },
  "homepage": "https://github.com/tristan-gscn/TCL-API-mirror#readme",
  "dependencies": {
    "axios": "^1.13.4",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.10",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^10.0.1",
    "eslint-config-prettier": "^10.1.8",
    "nodemon": "^3.1.11",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "ajv": "^8.18.0",
    "minimatch": "^10.2.1"
  }
}